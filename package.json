{
  "name": "@michelo11/botjs",
  "private": false,
  "version": "1.0.10",
  "description": "BotJS",
  "license": "MIT",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "botjs": "src/cli/index.cjs"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BotJSLib/lib.git"
  },
  "keywords": [
    "bot",
    "discord",
    "slack",
    "twitch",
    "telegram",
    "reddit",
    "whatsapp"
  ],
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.cjs",
      "require": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node-telegram-bot-api": "^0.64.2",
    "@types/tmi.js": "^1.8.6",
    "@types/yargs": "^17.0.32",
    "create-ts-index": "^1.14.0",
    "dotenv": "^16.4.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@discordx/importer": "^1.3.0",
    "@slack/bolt": "^3.17.1",
    "boxen": "^7.1.1",
    "chalk": "4.1.2",
    "chokidar": "^3.5.3",
    "cli-spinners": "^2.9.2",
    "discord.js": "^14.14.1",
    "express": "^4.18.2",
    "node-telegram-bot-api": "^0.64.0",
    "node-typescript-compiler": "^4.0.0",
    "ora": "^8.0.1",
    "snoowrap": "^1.23.0",
    "tmi.js": "^1.8.5",
    "tsx": "^4.7.0",
    "twilio": "^4.21.0",
    "twitch-js": "2.0.0-beta.45",
    "yargs": "^17.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
